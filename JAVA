import java.util.*;
 

public class ArrayShuffle {

    public static void main(String[] args)

    {

        ArrayList<Integer> mylist = new ArrayList<Integer>();

        mylist.add(1);

        mylist.add(2);

        mylist.add(3);

        mylist.add(4);

        mylist.add(5);

        mylist.add(6);
        mylist.add(7);
 

        System.out.println("Before Shuffle List : " + mylist);
 

        Collections.shuffle(mylist);
 

        System.out.println("After Shuffle List : " + mylist);

    }
}
